        "import matplotlib.pyplot as plt\n",
        "\n",
        "def generate_data(n_samples):\n",
        "    x = np.random.uniform(-1, 1, size=n_samples)\n",
        "    y = 0.2 * x**4 + 2 * x**3 + 0.1 * x**2 + 10\n",
        "    return x, y\n",
        "\n",
        "n_samples = 30000\n",
        "x, y = generate_data(n_samples)\n",
        "\n",
        "plt.scatter(x, y, s=1)\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('y')\n",
        "plt.show()"
